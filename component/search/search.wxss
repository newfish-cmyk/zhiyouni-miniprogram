/* component/search/search.wxss */
.my-search-container {
    background-color: #f2fdf2;
    height: 100rpx;
    padding: 0 20rpx;
    display: flex;
    align-items: center;
}
.my-search-box {
    height: 72rpx;
    background-color: white;
    border-radius: 30rpx;
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}
.placeholder {
    font-size: 32rpx;
    margin-left: 10rpx;
}
